                {{/each}}
                <div class="">
                    <p class="font-header text-xl font-bold">{{this.title}}</p>
                    <a href="#" class="font-followup font-semibold">{{this.category}}</a>
                    <p class="font-followup font-bold">{{this.discription}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
